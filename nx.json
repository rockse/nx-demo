{
  "npmScope": "stores",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "account": {
      "tags": []
    },
    "account-e2e": {
      "tags": [],
      "implicitDependencies": ["account"]
    },
    "brands": {
      "tags": []
    },
    "checkout": {
      "tags": []
    },
    "checkout-e2e": {
      "tags": [],
      "implicitDependencies": ["checkout"]
    },
    "header": {
      "tags": []
    },
    "shopping": {
      "tags": []
    },
    "shopping-e2e": {
      "tags": [],
      "implicitDependencies": ["shopping"]
    }
  }
}
